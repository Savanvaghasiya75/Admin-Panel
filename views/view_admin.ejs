          <div class="row">
              <div class="card">
                <div class="card-body">
                  <h5 class="card-title">USERS DETAILS</h5>
                  <div class="table-responsive">
                    <table id="zero_config" class="table table-striped table-bordered">
                      <thead>
                        <tr>
                          <th>NAME</th>
                          <th>EMAIL</th>
                          <th>GENDER</th>
                          <th>HOBBY</th>
                          <th>CITY</th>
                          <th>IMAGE</th>
                          <th>DESCRIPTION</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% for(var data of AllRecord){ %>
                         <tr>
                            <td><%= data.name %></td>
                            <td><%= data.email %></td>
                            <td><%= data.gender %></td>
                            <td><%= data.hobby %></td>
                            <td><%= data.city %></td>
                            <td><img src="<%= data.avatar %>" height="100" width="150"></td>
                            <td><%= data.description %></td>
                            <td><a href="/updatePage/?id=<%= data.id %>" style="padding: 3px 5px; color: #fff; background-color: #7460ee; border-color: #7460ee;">UPDATE</a></td>
                            <td><a href="/deleteData/?id=<%= data.id %>" style="padding: 3px 5px; color: #fff; background-color: red; border-color: #7460ee;">DELETE</a></td>
                          </tr>
                        <% } %>
                      </tbody>
                      <tfoot>
                      </tfoot>
                    </table>
                  </div>
                </div>
